{
    "name": "itstudioat/fresh-laravel",
    "description": "This is my package fresh-laravel",
    "keywords": [
        "itstudioat",
        "laravel",
        "fresh-laravel"
    ],
    "homepage": "https://github.com/itstudioat/fresh-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Guenther Kron",
            "email": "kron@naturwelt.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "dependencies": {
        "@vitejs/plugin-vue": "^5.0.4",
        "filepond": "^4.31.1",
        "filepond-plugin-file-validate-type": "^1.2.9",
        "filepond-plugin-image-preview": "^4.6.12",
        "kute.js": "^2.2.4",
        "pinia": "^2.1.7",
        "puppeteer": "^23.4.0",
        "sass-embedded": "^1.83.4",
        "shiki": "^1.20.0",
        "vite-plugin-vuetify": "^2.0.3",
        "vue": "^3.4.21",
        "vue-filepond": "^7.0.4",
        "vue-router": "^4.3.0",
        "vuetify": "^3.5.11"
    },
    "devDependencies": {
        "@vuepress/bundler-vite": "^2.0.0-rc.19",
        "@vuepress/plugin-markdown-image": "^2.0.0-rc.86",
        "@vuepress/theme-default": "^2.0.0-rc.74",
        "axios": "^1.6.4",
        "laravel-vite-plugin": "^1.0",
        "vite": "^5.0",
        "vuepress": "^2.0.0-rc.19"
    },
    "autoload": {
        "psr-4": {
            "Itstudioat\\FreshLaravel\\": "src/",
            "Itstudioat\\FreshLaravel\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itstudioat\\FreshLaravel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itstudioat\\FreshLaravel\\FreshLaravelServiceProvider"
            ],
            "aliases": {
                "FreshLaravel": "Itstudioat\\FreshLaravel\\Facades\\FreshLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}